<?xml version="1.0" encoding="UTF-8" ?>
<?demandware-pipeline version="2.0"?>

<pipeline group="Catalog">
  <branch basename="_ANONYMOUS_BRANCH_1">
    <segment>
      <node>
        <text-node>
          <description>Renders the home page.</description>
        </text-node>
        <node-display x="1" y="0"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_2">
    <segment>
      <node>
        <text-node>
          <description>Renders the whole menu to use as a remote include. It's cached.</description>
        </text-node>
        <node-display x="3" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="Show">
    <segment>
      <node>
        <start-node name="Show" secure="false"/>
        <node-display x="1" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <decision-node condition-key="('pageRetention' in CurrentSession.custom) &amp;&amp; !empty(CurrentSession.custom.pageRetention) &amp;&amp; CurrentCustomer.authenticated" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in">
            <transition-display>
              <bend-point relative-to="target" x="0" y="-1"/>
            </transition-display>
          </transition>
          <segment>
            <node>
              <jump-node start-name-ref="LoginBarrier-Redirect"/>
              <node-display x="-1" y="1"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition/>
      <node>
        <call-node start-name-ref="Home-UpdateMetaData"/>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <pipelet-node custom-name="Track the page being called" pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <key-binding alias="&quot;HomePage&quot;" key="From_0"/>
          <key-binding alias="CurrentSession.custom.callFrom" key="To_0"/>
          <key-binding alias="true" key="From_2"/>
          <key-binding alias="IsHomePage" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="CurrentSession.custom.pageRetention" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
          <key-binding alias="&quot;homepage&quot;" key="From_1"/>
          <key-binding alias="HomePage" key="To_1"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="content/home/homepage"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="IncludeHeader">
    <segment>
      <node>
        <start-node name="IncludeHeader" secure="false"/>
        <node-display x="3" y="2"/>
      </node>
      <simple-transition/>
      <node>
        <decision-node condition-key="CurrentSession.custom.device != 'mobile' || CurrentHttpParameterMap.isParameterSubmitted('MobileOptOut')" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in"/>
          <segment>
            <node>
              <interaction-node transaction-required="false">
                <template buffered="true" dynamic="false" name="components/header/header"/>
              </interaction-node>
              <node-display x="0" y="2"/>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="2" y="0"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <key-binding alias="!empty(CurrentCustomer) &amp;&amp; !empty(CurrentCustomer.profile) &amp;&amp;  'chainCode' in CurrentCustomer.profile.custom ? CurrentCustomer.profile.custom.chainCode : ''" key="From_0"/>
          <key-binding alias="cCode" key="To_0"/>
          <key-binding alias="!empty(CurrentCustomer) &amp;&amp; !empty(CurrentCustomer.profile) &amp;&amp;  'consolidationCode' in CurrentCustomer.profile.custom ? CurrentCustomer.profile.custom.consolidationCode : ''" key="From_1"/>
          <key-binding alias="conCode" key="To_1"/>
          <key-binding alias="null" key="From_2"/>
          <key-binding alias="null" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="1" y="1"/>
      </node>
      <simple-transition/>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="components/header/mobileheader"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_5">
    <segment>
      <node>
        <text-node>
          <description>Renders the category navigation and the menu to use as a remote include. It's cached.</description>
        </text-node>
        <node-display x="5" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_6">
    <segment>
      <node>
        <text-node>
          <description>This pipeline is used to bypass caching of the Chain and Consolidation Code so each cached templater will be unique per code group</description>
        </text-node>
        <node-display width="2" x="7" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="IncludeHeaderMenu">
    <segment>
      <node>
        <start-node name="IncludeHeaderMenu" secure="false"/>
        <node-display x="5" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="components/header/headermenu"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="IncludeCustomHeaderMenu">
    <segment>
      <node>
        <start-node name="IncludeCustomHeaderMenu" secure="false"/>
        <node-display x="7" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
          <config-property key="Transactional" value="false"/>
          <key-binding alias="!empty(CurrentCustomer) &amp;&amp; !empty(CurrentCustomer.profile) &amp;&amp;  'chainCode' in CurrentCustomer.profile.custom ? CurrentCustomer.profile.custom.chainCode : ''" key="From_0"/>
          <key-binding alias="cCode" key="To_0"/>
          <key-binding alias="!empty(CurrentCustomer) &amp;&amp; !empty(CurrentCustomer.profile) &amp;&amp;  'consolidationCode' in CurrentCustomer.profile.custom ? CurrentCustomer.profile.custom.consolidationCode : ''" key="From_1"/>
          <key-binding alias="conCode" key="To_1"/>
          <key-binding alias="null" key="From_2"/>
          <key-binding alias="null" key="To_2"/>
          <key-binding alias="null" key="From_3"/>
          <key-binding alias="null" key="To_3"/>
          <key-binding alias="null" key="From_4"/>
          <key-binding alias="null" key="To_4"/>
          <key-binding alias="null" key="From_5"/>
          <key-binding alias="null" key="To_5"/>
          <key-binding alias="null" key="From_6"/>
          <key-binding alias="null" key="To_6"/>
          <key-binding alias="null" key="From_7"/>
          <key-binding alias="null" key="To_7"/>
          <key-binding alias="null" key="From_8"/>
          <key-binding alias="null" key="To_8"/>
          <key-binding alias="null" key="From_9"/>
          <key-binding alias="null" key="To_9"/>
        </pipelet-node>
        <node-display x="0" y="1"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="components/header/customheadermenu"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="_ANONYMOUS_BRANCH_9">
    <segment>
      <node>
        <text-node>
          <description>Renders customer information. This is session information and must not be cached.</description>
        </text-node>
        <node-display x="10" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="IncludeHeaderCustomerInfo">
    <segment>
      <node>
        <start-node name="IncludeHeaderCustomerInfo" secure="false"/>
        <node-display x="10" y="2"/>
      </node>
      <simple-transition>
        <transition-display>
          <bend-point relative-to="source" x="0" y="1"/>
        </transition-display>
      </simple-transition>
      <node>
        <interaction-node transaction-required="false">
          <template buffered="true" dynamic="false" name="components/header/headercustomerinfo"/>
        </interaction-node>
        <node-display x="0" y="1"/>
      </node>
    </segment>
  </branch>
  <branch basename="HomeLogin">
    <segment>
      <node>
        <start-node name="HomeLogin" secure="false"/>
        <node-display x="14" y="0"/>
      </node>
      <simple-transition/>
      <node>
        <decision-node condition-key="!CurrentCustomer.authenticated" condition-operator="expr"/>
        <node-display x="0" y="1"/>
        <branch basename="b2" source-connector="yes">
          <transition target-connector="in"/>
          <segment>
            <node>
              <pipelet-node custom-name="Please note the backwards logic in From_1, it is correct, please do not change" pipelet-name="Assign" pipelet-set-identifier="bc_api">
                <config-property key="Transactional" value="false"/>
                <key-binding alias="('pageRetention' in CurrentSession.custom &amp;&amp; !empty(CurrentSession.custom.pageRetention)) ? false : true" key="From_0"/>
                <key-binding alias="setPageRetention" key="To_0"/>
                <key-binding alias="setPageRetention ? dw.web.URLUtils.https('Home-Show').toString() : CurrentSession.custom.pageRetention" key="From_1"/>
                <key-binding alias="CurrentSession.custom.pageRetention" key="To_1"/>
                <key-binding alias="null" key="From_3"/>
                <key-binding alias="null" key="To_3"/>
                <key-binding alias="null" key="From_4"/>
                <key-binding alias="null" key="To_4"/>
                <key-binding alias="null" key="From_5"/>
                <key-binding alias="null" key="To_5"/>
                <key-binding alias="null" key="From_6"/>
                <key-binding alias="null" key="To_6"/>
                <key-binding alias="null" key="From_7"/>
                <key-binding alias="null" key="To_7"/>
                <key-binding alias="null" key="From_8"/>
                <key-binding alias="null" key="To_8"/>
                <key-binding alias="null" key="From_9"/>
                <key-binding alias="null" key="To_9"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                <config-property key="Transactional" value="false"/>
                <key-binding alias="!empty(CurrentHttpParameterMap.pageFrom.stringValue) ? CurrentHttpParameterMap.pageFrom.stringValue :''" key="From_0"/>
                <key-binding alias="pageFrom" key="To_0"/>
                <key-binding alias="null" key="From_1"/>
                <key-binding alias="null" key="To_1"/>
                <key-binding alias="null" key="From_2"/>
                <key-binding alias="null" key="To_2"/>
                <key-binding alias="null" key="From_3"/>
                <key-binding alias="null" key="To_3"/>
                <key-binding alias="null" key="From_4"/>
                <key-binding alias="null" key="To_4"/>
                <key-binding alias="null" key="From_5"/>
                <key-binding alias="null" key="To_5"/>
                <key-binding alias="null" key="From_6"/>
                <key-binding alias="null" key="To_6"/>
                <key-binding alias="null" key="From_7"/>
                <key-binding alias="null" key="To_7"/>
                <key-binding alias="null" key="From_8"/>
                <key-binding alias="null" key="To_8"/>
                <key-binding alias="null" key="From_9"/>
                <key-binding alias="null" key="To_9"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <decision-node condition-key="!empty(pageFrom)" condition-operator="expr"/>
              <node-display x="0" y="1"/>
              <branch basename="b2" source-connector="yes">
                <transition target-connector="in"/>
                <segment>
                  <node>
                    <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                      <config-property key="Transactional" value="false"/>
                      <key-binding alias="dw.web.URLUtils.https('PLCC-LandingPage','pfrm','login').toString()" key="From_0"/>
                      <key-binding alias="CurrentSession.custom.pageRetention" key="To_0"/>
                      <key-binding alias="null" key="From_1"/>
                      <key-binding alias="null" key="To_1"/>
                      <key-binding alias="null" key="From_2"/>
                      <key-binding alias="null" key="To_2"/>
                      <key-binding alias="null" key="From_3"/>
                      <key-binding alias="null" key="To_3"/>
                      <key-binding alias="null" key="From_4"/>
                      <key-binding alias="null" key="To_4"/>
                      <key-binding alias="null" key="From_5"/>
                      <key-binding alias="null" key="To_5"/>
                      <key-binding alias="null" key="From_6"/>
                      <key-binding alias="null" key="To_6"/>
                      <key-binding alias="null" key="From_7"/>
                      <key-binding alias="null" key="To_7"/>
                      <key-binding alias="null" key="From_8"/>
                      <key-binding alias="null" key="To_8"/>
                      <key-binding alias="null" key="From_9"/>
                      <key-binding alias="null" key="To_9"/>
                    </pipelet-node>
                    <node-display x="0" y="1"/>
                  </node>
                  <transition target-connector="in1" target-path="../+1"/>
                </segment>
              </branch>
            </node>
            <transition target-connector="in2" target-path="./+1"/>
          </segment>
          <segment>
            <node>
              <join-node/>
              <node-display x="0" y="2"/>
            </node>
            <simple-transition/>
            <node>
              <pipelet-node custom-name="CurrentForms.login" pipelet-name="ClearFormElement" pipelet-set-identifier="bc_api">
                <key-binding alias="CurrentForms.login" key="FormElement"/>
              </pipelet-node>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <call-node start-name-ref="LoginBarrier-SetLoginIDFromCookie"/>
              <node-display x="0" y="1"/>
            </node>
            <transition target-connector="in1" target-path="./+1"/>
          </segment>
          <segment>
            <node>
              <join-node/>
              <node-display x="0" y="1"/>
            </node>
            <simple-transition/>
            <node>
              <interaction-continue-node secure="true" start-name="Header Login" transaction-required="false">
                <template buffered="true" dynamic="false" name="components/barrier/headerbarrier"/>
              </interaction-continue-node>
              <node-display x="0" y="1"/>
              <branch basename="b2" source-connector="login">
                <transition target-connector="in"/>
                <segment>
                  <node>
                    <decision-node condition-key="dw.web.CSRFProtection.validateRequest()" condition-operator="expr"/>
                    <node-display x="0" y="1"/>
                    <branch basename="b2" source-connector="yes">
                      <transition target-connector="in"/>
                      <segment>
                        <node>
                          <call-node start-name-ref="Login-Process"/>
                          <node-display x="0" y="1"/>
                          <branch basename="b2" source-connector="login_failed">
                            <transition target-connector="in1" target-path="../+1"/>
                          </branch>
                          <branch basename="b3" source-connector="login_succeeded">
                            <transition target-connector="in"/>
                            <segment>
                              <node>
                                <call-node start-name-ref="Cart-GetBasket"/>
                                <node-display x="0" y="1"/>
                              </node>
                              <simple-transition/>
                              <node>
                                <pipelet-node pipelet-name="Script" pipelet-set-identifier="bc_api">
                                  <config-property key="Transactional" value="true"/>
                                  <config-property key="OnError" value="PIPELET_ERROR"/>
                                  <config-property key="ScriptFile" value="cart/updateBasketSDD.ds"/>
                                  <key-binding alias="ScriptLog" key="ScriptLog"/>
                                  <key-binding alias="Basket" key="Basket"/>
                                  <key-binding alias="CurrentCustomer" key="Customer"/>
                                  <key-binding alias="CurrentHttpParameterMap.pli.stringValue" key="Pli"/>
                                  <key-binding alias="CurrentHttpParameterMap.qty.intValue" key="Quantity"/>
                                  <key-binding alias="isSDDRequired" key="isSDDRequired"/>
                                  <key-binding alias="CurrentCustomer" key="CurrentCustomer"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                                <branch basename="b2" source-connector="error">
                                  <transition target-connector="in2" target-path="./+1">
                                    <transition-display>
                                      <bend-point relative-to="source" x="1" y="0"/>
                                      <bend-point relative-to="target" x="1" y="0"/>
                                    </transition-display>
                                  </transition>
                                </branch>
                              </node>
                              <transition target-connector="in1" target-path="./+1"/>
                            </segment>
                            <segment>
                              <node>
                                <join-node/>
                                <node-display x="0" y="1"/>
                              </node>
                              <simple-transition/>
                              <node>
                                <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                                  <key-binding alias="true" key="From_0"/>
                                  <key-binding alias="LoginSuccess" key="To_0"/>
                                  <key-binding alias="delete CurrentSession.custom.isMobileLogin" key="From_1"/>
                                  <key-binding alias="doIt" key="To_1"/>
                                  <key-binding alias="true" key="From_2"/>
                                  <key-binding alias="serviceCallwithoutZipcode" key="To_2"/>
                                </pipelet-node>
                                <node-display x="0" y="1"/>
                              </node>
                              <simple-transition/>
                              <node>
                                <call-node start-name-ref="Account-AccountSync"/>
                                <node-display x="0" y="1"/>
                                <branch basename="b2" source-connector="error">
                                  <transition target-connector="in1"/>
                                  <segment>
                                    <node>
                                      <join-node/>
                                      <node-display x="1" y="-4"/>
                                    </node>
                                    <transition target-connector="in1" target-path="../+1"/>
                                  </segment>
                                </branch>
                              </node>
                              <transition target-connector="in1" target-path="./+1"/>
                            </segment>
                            <segment>
                              <node>
                                <join-node/>
                                <node-display x="0" y="1"/>
                              </node>
                              <simple-transition/>
                              <node>
                                <call-node start-name-ref="Login-AssignDiscountModelAndPricebooks"/>
                                <node-display x="0" y="1"/>
                                <branch basename="b2" source-connector="login_succeeded">
                                  <transition target-connector="in"/>
                                  <segment>
                                    <node>
                                      <call-node start-name-ref="LoginBarrier-GetShippingAddresses"/>
                                      <node-display x="0" y="1"/>
                                      <branch basename="b2" source-connector="one">
                                        <transition target-connector="in1" target-path="./+1"/>
                                      </branch>
                                    </node>
                                    <simple-transition/>
                                    <node>
                                      <pipelet-node pipelet-name="Assign" pipelet-set-identifier="bc_api">
                                        <config-property key="Transactional" value="false"/>
                                        <key-binding alias="true" key="From_0"/>
                                        <key-binding alias="showAddressSelection" key="To_0"/>
                                        <key-binding alias="null" key="From_1"/>
                                        <key-binding alias="null" key="To_1"/>
                                        <key-binding alias="null" key="From_2"/>
                                        <key-binding alias="null" key="To_2"/>
                                        <key-binding alias="null" key="From_3"/>
                                        <key-binding alias="null" key="To_3"/>
                                        <key-binding alias="null" key="From_4"/>
                                        <key-binding alias="null" key="To_4"/>
                                        <key-binding alias="null" key="From_5"/>
                                        <key-binding alias="null" key="To_5"/>
                                        <key-binding alias="null" key="From_6"/>
                                        <key-binding alias="null" key="To_6"/>
                                        <key-binding alias="null" key="From_7"/>
                                        <key-binding alias="null" key="To_7"/>
                                        <key-binding alias="null" key="From_8"/>
                                        <key-binding alias="null" key="To_8"/>
                                        <key-binding alias="null" key="From_9"/>
                                        <key-binding alias="null" key="To_9"/>
                                      </pipelet-node>
                                      <node-display x="0" y="1"/>
                                    </node>
                                    <transition target-connector="in1" target-path="./+1">
                                      <transition-display>
                                        <bend-point relative-to="target" x="0" y="7"/>
                                      </transition-display>
                                    </transition>
                                  </segment>
                                  <segment>
                                    <node>
                                      <join-node/>
                                      <node-display x="2" y="-9"/>
                                    </node>
                                    <simple-transition>
                                      <transition-display>
                                        <bend-point relative-to="source" x="0" y="-1"/>
                                      </transition-display>
                                    </simple-transition>
                                    <node>
                                      <pipelet-node custom-name="Delete TempChainCode" pipelet-name="Assign" pipelet-set-identifier="bc_api">
                                        <description>Delete 'TempChainCode' to prevent portal pages from displaying incorrect content slot configurations. JIRA Ticket Reference CPS-662 for more information.</description>
                                        <config-property key="Transactional" value="false"/>
                                        <key-binding alias="doIt" key="To_0"/>
                                        <key-binding alias="delete CurrentSession.custom.tempChainCode" key="From_0"/>
                                      </pipelet-node>
                                      <node-display orientation="horizontal" x="0" y="-3"/>
                                    </node>
                                    <transition target-connector="in2" target-path="../../../+1">
                                      <transition-display>
                                        <bend-point relative-to="source" x="1" y="0"/>
                                        <bend-point relative-to="source" x="1" y="-2"/>
                                        <bend-point relative-to="target" x="1" y="0"/>
                                      </transition-display>
                                    </transition>
                                  </segment>
                                </branch>
                              </node>
                            </segment>
                          </branch>
                        </node>
                      </segment>
                    </branch>
                  </node>
                  <transition target-connector="in1" target-path="./+1"/>
                </segment>
                <segment>
                  <node>
                    <join-node/>
                    <node-display x="2" y="-4"/>
                  </node>
                  <transition target-connector="in1" target-path=".."/>
                </segment>
              </branch>
            </node>
          </segment>
        </branch>
      </node>
      <simple-transition>
        <transition-display>
